{
	"name": "brotkrueml/typo3-feed-generator",
	"description": "Generator for Atom, JSON and RSS feeds in TYPO3",
	"license": "GPL-2.0-or-later",
	"type": "typo3-cms-extension",
	"keywords": [
		"TYPO3 CMS",
		"TYPO3 extension",
		"Feed",
		"Atom",
		"RSS"
	],
	"authors": [
		{
			"name": "Chris MÃ¼ller",
			"role": "Developer"
		}
	],
	"support": {
		"source": "https://github.com/brotkrueml/typo3-feed-generator"
	},
	"require": {
		"php": ">=8.1",
		"composer/semver": "^3.0",
		"typo3/cms-core": "^11.5 || ^12.0"
	},
	"require-dev": {
		"brotkrueml/coding-standards": "~2.0.2",
		"brotkrueml/t3docs-codesnippets-standalone": "dev-main@dev",
		"ergebnis/composer-normalize": "~2.28.0",
		"jdecool/jsonfeed": "dev-master",
		"laminas/laminas-feed": "^2.19",
		"php-coveralls/php-coveralls": "^2.5",
		"phpstan/extension-installer": "^1.1",
		"phpstan/phpstan": "1.9.1",
		"phpunit/phpunit": "^9.5.23",
		"rector/rector": "0.14.6",
		"symfony/yaml": "^5.4 || ^6.1",
		"symplify/phpstan-rules": "^10.1",
		"typo3/cms-lowlevel": "^11.5 || ^12.0",
		"typo3/cms-reports": "^11.5 || ^12.0"
	},
	"suggest": {
		"jdecool/jsonfeed": "Required for generating JSON feeds",
		"laminas/laminas-feed": "Required for generating Atom and RSS feeds",
		"typo3/cms-lowlevel": "Display available feeds in Configurations module",
		"typo3/cms-reports": "Displays status report for feed generator"
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/brotkrueml/t3docs-codesnippets-standalone"
		}
	],
	"autoload": {
		"psr-4": {
			"Brotkrueml\\FeedGenerator\\": "Classes"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Brotkrueml\\FeedGenerator\\Tests\\": "Tests"
		}
	},
	"config": {
		"allow-plugins": {
			"ergebnis/composer-normalize": true,
			"typo3/class-alias-loader": true,
			"typo3/cms-composer-installers": true,
			"phpstan/extension-installer": true
		},
		"bin-dir": ".Build/bin",
		"sort-packages": true,
		"vendor-dir": ".Build/vendor"
	},
	"extra": {
		"branch-alias": {
			"dev-main": "0.4.x-dev"
		},
		"typo3/cms": {
			"app-dir": ".Build",
			"extension-key": "feed_generator",
			"web-dir": ".Build/web"
		}
	}
}
